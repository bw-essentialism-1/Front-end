{
    "version": 2,
    "name": "Front-end",
    "builds": [
      {
        "src": "/package.json",
        "use": "@now/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Front-end/$1"
      }
    ]
  }